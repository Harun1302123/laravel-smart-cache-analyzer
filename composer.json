{
    "name": "harun1302123/laravel-smart-cache-analyzer",
    "description": "Intelligent cache analysis and optimization for Laravel applications",
    "keywords": [
        "laravel",
        "cache",
        "analyzer",
        "performance",
        "optimization",
        "analytics"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MD HARUNUR RASHID",
            "email": "ahmharunurrashid@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SmartCache\\Analyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartCache\\Analyzer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartCache\\Analyzer\\SmartCacheServiceProvider"
            ],
            "aliases": {
                "SmartCache": "SmartCache\\Analyzer\\Facades\\SmartCache"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
